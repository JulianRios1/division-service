# Configuración de entorno para division-service
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
PORT=8081

# Google Cloud Platform
GOOGLE_CLOUD_PROJECT=your-project-id
GCP_REGION=us-central1

# Credenciales de desarrollo local (solo para desarrollo)
# Crear service account en GCP Console y descargar el JSON
GOOGLE_APPLICATION_CREDENTIALS=./credentials/service-account-key.json

# Base de datos
DB_HOST=localhost
DB_PORT=5432
DB_NAME=shipments_db
DB_USER=postgres
DB_PASSWORD=your-password

# Logging
LOG_LEVEL=INFO

# Division Service específico
BUCKET_JSON_PENDIENTES=your-project-json-pendientes
BUCKET_JSON_A_PROCESAR=your-project-json-a-procesar
MAX_SHIPMENTS_PER_FILE=100
IMAGE_PROCESSING_SERVICE_URL=https://image-processing-service-url
EMAIL_SERVICE_URL=https://email-service-url
